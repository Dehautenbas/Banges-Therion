encoding  utf-8

survey FE_tot -title "Gouffre du Fitoja-Express"
input FE_map.th

    #input datas/FE.th    
    input datas/entree-jonction/entree-jonction_tot.th
    input datas/jonction-meduse-haute/jonction-meduse-haute_tot.th
