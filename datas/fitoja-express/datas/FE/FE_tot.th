             #join SP1_fossile-sup2@fossile-sup2_tot SP1_fossile-sup@fossile-sup_tot    #3vis√©es Martin qui ne tombent pas en face
             join SP2_fossile-sup2@fossile-sup2_tot SP1_jonction-meduse-haute@jonction-meduse-haute_tot
             join L1_SP2_jonction-meduse-haute@jonction-meduse-haute_tot:end L1_SP2_fossile-sup2@fossile-sup2_tot:0
             join L2_SP2_jonction-meduse-haute@jonction-meduse-haute_tot:0 L2_SP2_fossile-sup2@fossile-sup2_tot:end
    input datas/fossile-aval/fossile-aval_tot.th
        equate MEDU18@fossile-aval.fossile-aval_tot MEDU18@jonction-meduse-haute.jonction-meduse-haute_tot
            join SP1_fossile-aval@fossile-aval_tot SP1_jonction-meduse-haute@jonction-meduse-haute_tot
    input datas/col-canyon-vers-meduse/col-canyon-vers-meduse_tot.th
