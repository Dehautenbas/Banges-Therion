        equate MEDU8@meduse-inf.meduse-inf_tot 222@boyau-rd-jonction.boyau-rd-jonction_tot
        equate FE52@jonction-meduse-haute.jonction-meduse-haute_tot 42@boyau-rd-jonction.boyau-rd-jonction_tot
            join SP1_jonction-meduse-haute@jonction-meduse-haute_tot SP1_boyau-rd-jonction@boyau-rd-jonction_tot
    input datas/meduse-inf/meduse-inf_tot.th
        equate MEDU7@jonction-meduse-haute.jonction-meduse-haute_tot MEDU7@meduse-inf.meduse-inf_tot
    input datas/fossile-sup/fossile-sup_tot.th
        equate MEDU23@fossile-sup.fossile-sup_tot MEDU23@jonction-meduse-haute.jonction-meduse-haute_tot
        equate MEDU26@fossile-sup.fossile-sup_tot MEDU26@jonction-meduse-haute.jonction-meduse-haute_tot
    input datas/fossile-sup2/fossile-sup2_tot.th
